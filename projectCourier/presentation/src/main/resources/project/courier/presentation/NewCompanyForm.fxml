   <children>
      <Label layoutX="242.0" layoutY="65.0" text="New Company Form" />
      <Label layoutX="168.0" layoutY="114.0" text="Name" textAlignment="RIGHT" />
      <TextField fx:id="companyName" layoutX="257.0" layoutY="110.0" />
      <Label layoutX="168.0" layoutY="150.0" text="Bulstat" textAlignment="RIGHT" />
      <TextField fx:id="bulstat" layoutX="256.0" layoutY="146.0" />
      <Button layoutX="256.0" layoutY="257.0" mnemonicParsing="false" onAction="#registerCompany" text="Register" />
      <Button layoutX="351.0" layoutY="257.0" mnemonicParsing="false" onAction="#closeForm" fx:id="closeButton" text="Cancel" />
   </children>
</AnchorPane>
