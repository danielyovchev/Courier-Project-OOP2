<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.courier.presentation.controller.AdminController">
   <children>
      <MenuBar layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="459.0">
        <menus>
          <Menu mnemonicParsing="false" text="Register new company">
            <items>
<!--                <fx:include source="NewCompanyForm.fxml"/>-->
                <MenuItem fx:id="regCompany" mnemonicParsing="false" text="Register Company" onAction="#openCompanyRegForm"/>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Register new courier">
            <items>

<!--                <fx:include source="NewCourierForm.fxml"/>-->
                <MenuItem fx:id="regCourie" mnemonicParsing="false" text="Register Courier" onAction="#openCourierRegForm"/>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Register new client" onAction="#openCustomerRegForm">
            <items>
<!--                <fx:include source="NewClientForm.fxml"/>-->
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Button layoutX="511.0" layoutY="344.0" mnemonicParsing="false" text="Log out" />
   </children>
</AnchorPane>
